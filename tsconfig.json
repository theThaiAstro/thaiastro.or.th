{
	"include": ["./src/**/*", "gatsby-browser.js"],
	"compilerOptions": {
		"target": "esnext",
		"module": "esnext",
		"lib": ["dom", "es2017"],
		"jsx": "react",
		"strict": true,
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"noEmit": true,
		"skipLibCheck": true,
		"moduleResolution": "node"
	}
}
